protocol demo_protocol {
	message start_sess {
		u8[2] HDR = [DEh ADh]
		u8[32] ID
		check-sum u16 CSUM
	}
	
	message ping {
		u8[2] HDR = [DEh EDh]
		u32   ADDR
		check-sum u8 CSUM
	}
	
	message pong {
		u8[2] HDR = [FEh EDh]
		check-sum u8 CSUM
	}
	
	message keyb_event {
		u8[2] HDR = [CAh FEh]
		
		enum u8 EVENT { // Command
			KEY_DOWN 01h
			KEY_UP   02h
			KEY_REPEAT FFh
		}
		
		bit-mask u8 MODIF { SHIFT:1, ALT:1, CTRL:1, RSV:5 }
		u8 KEY
		u16 X
		check-sum u16 CSUM
	}
	
	message terminate_sess {
		u8[2] HDR = [BEh EFh]
		u8 KEY
		u16 X
		check-sum u32 CSUM1
		check-sum u32 CSUM2
	}
}
